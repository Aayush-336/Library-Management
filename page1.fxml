<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="mainPane" prefHeight="628.0" prefWidth="865.0"
            stylesheets="@cssfile.css" xmlns="http://javafx.com/javafx/8" fx:controller="Page1">
    <children>
        <Button layoutX="135.0" layoutY="14.0" mnemonicParsing="false" text="Remove Book"/>
        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#addBook" text="Add Book"/>
        <Pane fx:id="changepane" layoutY="58.0" prefHeight="600.0" prefWidth="875.0" styleClass="changePane">
            <children>
                <Label layoutX="103.0" text="Page 1">
                    <font>
                        <Font size="42.0"/>
                    </font>
                </Label>
                <Label fx:id="label_user" layoutX="85.0" layoutY="211.0" text="Label"/>
                <Button layoutX="16.0" layoutY="140.0" mnemonicParsing="false" onAction="#getuserpass"
                        text="Get username and Password"/>
            </children>
        </Pane>
    </children>
</AnchorPane>
